<div id="<%= dom_id expense %>">
  <p>
    <strong>Name:</strong>
    <%= expense.name %>
  </p>

  <p>
    <strong>Amount:</strong>
    $<%= expense.amount %>
  </p>

  <p>
    <strong>Category: </strong>
    <%=  @group.name %>
  </p>

  <p>
  <strong>Created:</strong>
  <%= expense.created_at.strftime('%B %d, %Y') %>
  </p>
  
</div>
